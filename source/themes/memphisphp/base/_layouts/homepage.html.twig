                            <div class="caption">Joe Ferguson giving a talk at MemphisPHP</div>
                        </div>
                    </li>
                </ul>
            </div>
            <script type="text/javascript">
                var metaslider_20 = function($) {
                    $("#metaslider_20").responsiveSlides({
                        timeout:3000,
                        pager:false,
                        nav:true,
                        pause:true,
                        speed:600,
                        prevText:"&lt;",
                        nextText:"&gt;",
                        auto:true
                    });
                };
                var timer_metaslider_20 = function() {
                    var slider = !window.jQuery ? window.setTimeout(timer_metaslider_20, 100) : !jQuery.isReady ? window.setTimeout(timer_metaslider_20, 1) : metaslider_20(window.jQuery);
                };
                timer_metaslider_20();
            </script>
        </div>
        <!--// meta slider-->
    </div>

    <div id="main" class="site-main">
        <div class="full-container">

            <div id="primary" class="content-area">
                {% block content %}

                {% endblock %}
            </div><!-- #primary .content-area -->


            <div id="secondary" class="widget-area" role="complementary">
                <aside id="vsmeetsinglewidget-2" class="widget widget_vsmeetsinglewidget"><h3 class="widget-title">Next Meetup</h3>
                    <h3 class="event-title">
                        <a href="https://www.meetup.com/Memphis-PHP-Meetup/events/235161675/">
                            Memphis PHP UG Sticker Exchange & Laravel Discussion
                        </a>
                    </h3>
                    <p class="event-date">November 22nd, 2015 @ 6:30 pm</p>
                    <p class="event-summary">TBD</p>
                    <p class="event-rsvp">
                        <span class="rsvp-count"></span>
                        <span class="rsvp-add">
                            <a href="https://www.meetup.com/Memphis-PHP-Meetup/events/235161675/">RSVP</a>
                        </span>
                    </p>
                    <p class="event-location">
                        Location: <a href="http://maps.google.com/maps?q=Vaco MidSouth 6000 Poplar Ave Ste 216, Memphis, +%28Vaco MidSouth%29&z=17">
                            Vaco Midsouth 6000 Poplar Ave Ste 216, Memphis, TN
                        </a>
                    </p>
                </aside>
                <aside id="recent-posts-2" class="widget widget_recent_entries">
                    <h3 class="widget-title">Recent Posts</h3>
                    <ul>
                        {% for post in page.pagination.items %}
