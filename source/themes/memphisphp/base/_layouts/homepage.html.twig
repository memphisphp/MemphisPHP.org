                        <p>
                            <a href="http://www.vaco.com/memphis" target="_blank">
                                <img class="alignnone size-full wp-image-24" alt="vaco" src="/{{ theme_path('assets/vaco.png') }}" width="120" height="14" />
                            </a>
                        </p>
                        {#<p>#}
                            {#<a href="http://www.modernservicesinc.com/" target="_blank">#}
                            {#<img class="alignnone size-full wp-image-26" alt="modernservicesinc" src="/{{ theme_path('assets/modernservicesinc.png') }}" width="120" height="51" />#}
                            {#</a>#}
                        {#</p>#}
